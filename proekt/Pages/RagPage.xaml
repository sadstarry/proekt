<Page x:Class="proekt.Pages.RagPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:proekt.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="RagPage">

    <Grid Background="White">
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
            <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" Margin="5">
                <TextBlock Width="59" ><Run Text="Login"/></TextBlock>
                <TextBox x:Name="TbLoginReg" RenderTransformOrigin="0.5,0.5" Width="209">
                </TextBox>
            </StackPanel>
            <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" Margin="5">
                <TextBlock Width="59"><Run Text="Password"/></TextBlock>
                <TextBox x:Name="TbPasswordReg" Width="209"></TextBox>
            </StackPanel>
            <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" Margin="5">
                <TextBlock Width="59"><Run Text="Name"/></TextBlock>
                <TextBox x:Name="TbNameReg" Width="209" TextChanged="TbNameReg_TextChanged"></TextBox>
            </StackPanel>
            <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" Margin="5">
                <TextBlock Width="59"><Run Text="Surname"/></TextBlock>
                <TextBox x:Name="TbSurnameReg" Width="209" TextChanged="TbSurnameReg_TextChanged"></TextBox>
            </StackPanel>
            <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" Margin="5">
                <TextBlock Width="59"><Run Text="Patronymic"/></TextBlock>
                <TextBox x:Name="TbPatronymicReg" Width="209" TextChanged="TbPhoneReg_TextChanged"></TextBox>
            </StackPanel>
            <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" Margin="5">
                <TextBlock Width="59"><Run Text="Email"/></TextBlock>
                <TextBox x:Name="TbEmailReg" Width="209" TextChanged="TbEmailReg_TextChanged"></TextBox>
            </StackPanel>
            <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" Margin="5">
                <TextBlock Width="59"><Run Text="Phone"/></TextBlock>
                <TextBox x:Name="TbPhoneReg" Width="209" TextChanged="TbPhoneReg_TextChanged"></TextBox>
            </StackPanel>
            <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" Margin="5">
                <Button Click="Reg_click" Content="Registations " Width="169"/>
                <Button Click="ClearReg_click" Content="Clear" Width="169"/>
                <Button Click="NextReg_click" Content="Back" Width="169"/>
            </StackPanel>
        </StackPanel>
    </Grid>
</Page>
